    <section class="portfolio padding-tb" id="Portfolio">
      <div class="container">
        <div class="portfolio-top">
          <div class="portfolio-left">
            <div class="section-header">
              <div class="title">
                <span class="category wow fadeInUp" data-wow-duration="1s" data-wow-delay=".1s">Thư viện ảnh</span>
              </div>
            </div>
          </div>
          <div class="portfolio-right wow fadeInUp" data-wow-duration="1s" data-wow-delay=".3s"></div>
        </div>
        <div class="section-wrapper">
          <div class="grid wow fadeInUp" data-wow-duration="1s" data-wow-delay=".4s">
            <% gallerys.forEach(function(item) { %>
            <div class="grid-item g-img %>">
              <div class="grid-inner">
                <div class="thumb"></div>
                <img width="500px" src="\uploads\gallery\<%= item.imagePath %>" alt="<%= item.title || 'gallery item' %>">
                <div class="thumb-icon">
                  <a href="\uploads\gallery\<%= item.imagePath %>" data-rel="lightcase:myCollection"></a>
                </div>
              </div>
            </div>
            <% }); %>
          </div>
        </div>
      </div>
    </section>

   <section class="testimonial padding-tb" id="Clients" style="background-image: url(/client/assets/images/testimonial/bg.png);">
  <div class="particles" id="particles-js2"></div>
  <div class="container">
    <div class="section-header">
      <div class="title">
        <span class="category wow fadeInUp" data-wow-duration="1s" data-wow-delay=".1s">Lịch diễn</span>
        <h2 class="wow fadeInUp" data-wow-duration="1s" data-wow-delay=".2s">Các buổi biểu diễn <span>tại Nhà hát Cải lương Hà Nội</span></h2>
      </div>
    </div>
    <div class="section-wrapper wow fadeInUp" data-wow-duration="1s" data-wow-delay=".3s">
      <div class="testimonial-slider">
        <div class="swiper-wrapper">
          <% events.forEach(function(item) { %>
          <div class="swiper-slide" style="max-width: 100%; box-shadow: 0px 5px 15px rgba(0, 0, 0, 0.15);">
            <div class="testimonial-item" style="width: 100%; margin: 0 auto;">
              <div class="testimonial-inner">
                <div class="testimonial-head">
                  <img src="/uploads/events/<%= item.main_image %>" alt="" style="height: 300px !important; border-radius: 3%; width: 100%; object-fit: cover;">
                </div>
                <div class="testimonial-body">
                  <h3 style="background: linear-gradient(to right, #ffc802, #f80858); -webkit-background-clip: text; -webkit-text-fill-color: transparent; background-clip: text;"><%= item.name %></h3>
                  <p><%= item.description %></p>
                  <h5> <b>Giá vé:</b> <span style="font-size: larger; background: linear-gradient(to right, #fed643, #ff4181); -webkit-background-clip: text; -webkit-text-fill-color: transparent; background-clip: text;"><%= item.min_price.toLocaleString('vi-VN') %> đ - <%= item.max_price.toLocaleString('vi-VN') %> đ</span> </h5>
                </div>
                <div class="testimonial-footer">
                  <div class="text-center">
                    <a href="/deatail/events/<%= item.id %>" class="btn"><span>Chi tiết</span><i class="icofont-rounded-double-right"></i></a>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <%})%>

        </div>
      </div>
    </div>
  </div>
